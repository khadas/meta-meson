<mixer>
  <ctl name="HDMI ARC Switch" value="0" />

  <path name="speaker">
    <ctl name="SPK mute" value="0" />
  </path>

  <path name="speaker_off">
    <ctl name="SPK mute" value="1" />
  </path>

  <path name="hdmi_arc">
   <ctl name="HDMI ARC Switch" value="1" />
  </path>

  <path name="hdmi_arc_off">
   <ctl name="HDMI ARC Switch" value="0" />
  </path>

  <path name="headphone">
  </path>

  <path name="main_mic">
  </path>

  <path name="headset-mic">
  </path>

  <path name="line_in">
    <ctl name="TDMIN_B source select" value="acodec_adc" />
  </path>

  <path name="hdmirx_in">
    <ctl name="TDMIN_B source select" value="hdmirx" />
  </path>
</mixer>
