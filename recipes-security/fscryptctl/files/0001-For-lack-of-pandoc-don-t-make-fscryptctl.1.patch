From 321bb73cfd7f64a85d19553f13174e973ccff88a Mon Sep 17 00:00:00 2001
From: Mingyen Hung <mingyen.hung@amlogic.com>
Date: Thu, 25 Aug 2022 01:00:09 -0700
Subject: [PATCH] For lack of pandoc, don't make fscryptctl.1.

Signed-off-by: Mingyen Hung <mingyen.hung@amlogic.com>
---
 Makefile | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

Index: git/Makefile
===================================================================
--- git.orig/Makefile
+++ git/Makefile
@@ -133,7 +133,7 @@ test-all:
 
 # Installation, uninstallation, and cleanup targets
 
-all:fscryptctl fscryptctl.1
+all:fscryptctl
 
 .PHONY: all install install-bin install-man uninstall clean
 
@@ -145,7 +145,7 @@ install-man: fscryptctl.1
 	install -d $(DESTDIR)$(MANDIR)/man1
 	install -m644 $< $(DESTDIR)$(MANDIR)/man1
 
-install:install-bin install-man
+install:install-bin
 
 uninstall:
 	rm -f $(DESTDIR)$(BINDIR)/fscryptctl
